# Plot

library(plot3D)
library(rgl)
library(RColorBrewer)

x <- train_rfe$LCirc
y <- train_rfe$LTD
z <- train_rfe$LA

cols <- brewer.pal(8, "Accent")

cols

mycolors <- c("#7FC97F","#BEAED4","#FDC086","#FFFF99","#386CB0","#F0027F","#BF5B17","#666666")



s3 <- scatter3D(x,y,z,theta = 40,phi = 20,xlab= "LCirc",ylab= "LTD", zlab = "LA",
          bty="g",pch= 17,colors=cols,ticktype = "detailed",
          side=1,addlines=TRUE,length=0.5,width=0.5,
          main="South eastern perennial") 

legend("right",legend = levels(train_rfe$Species),
       col =  cols, pch = 16)



data <- train_rfe

cols <- brewer.pal(8, "Accent")

cols

mycolors <- c("red","blue","orange","pink","brown","purple","violet","yellow")

data$color <- mycolors[as.numeric(data$Species)]

# Plot
plot3d( 
  x=data$`Sepal.Length`, y=data$`Sepal.Width`, z=data$`Petal.Length`, 
  col = data$color, 
  type = 's', 
  radius = .1,
  xlab="Sepal Length", ylab="Sepal Width", zlab="Petal Length")





movie3d(spin3d(axis = c(0,0,1),rpm = 2,),duration = 60,dir = "./")


################# 3D PLOT BASED ON THE MOST DIVERGENT TRAITS ###### 


library(plotly)

plot <- plot_ly(train_new,x= ~LCirc,y= ~LTD,z= ~LA, color = ~Species
) %>%
  add_markers() %>%
  layout(
    scene = list(xaxis = list(title = 'LCirc'),
                 yaxis = list(title = 'LTD'),
                 zaxis = list(title = 'LA'))
  )


plot




library(htmlwidgets)

